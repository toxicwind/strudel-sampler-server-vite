{
  "name": "strudel-sampler-microservice",
  "version": "1.0.0",
  "description": "Production-ready Strudel Sampler microservice with Vite plugin integration",
  "type": "module",
  "main": "dist/vite-plugin-strudel-sampler.js",
  "exports": {
    ".": {
      "import": "./dist/vite-plugin-strudel-sampler.js",
      "types": "./dist/vite-plugin-strudel-sampler.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "sampler": "tsx sampler-server.ts",
    "sampler:prod": "node dist/sampler-server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "strudel",
    "sampler",
    "vite",
    "plugin",
    "microservice",
    "music",
    "live-coding",
    "production"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "music-metadata": "^8.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}